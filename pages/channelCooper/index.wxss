.channel-cooper-page {
  display: flex;
  flex-direction: column;
  background: #fff;
  align-items: center;
  padding-top: 226rpx;
  min-height: 100vh;
}

.channel-cooper-page .msg {
  display: flex;
  flex-direction: column;
  text-align: center;
}

.channel-cooper-page .msg text {
  font-size: 28rpx;
  font-family: PingFang SC-Medium, PingFang SC;
  font-weight: 500;
  color: #000000;
}

.channel-cooper-page .msg text:last-of-type {
  margin-top: 10rpx;
}

.channel-cooper-page .input {
  margin-top: 20rpx;
}

.channel-cooper-page .input input {
  width: 640rpx;
  height: 92rpx;
  border-radius: 10rpx;
  background: #f8f8f8;
  padding: 0 20rpx;
}

.channel-cooper-page button {
  margin-top: 190rpx;
  width: 440rpx;
  height: 76rpx;
  line-height: 76rpx;
  padding: 0;
  border-radius: 10rpx;
  background: var(--liner-bg-Color);
  color: #fff;
  font-size: 26rpx;
  font-family: PingFang SC-Semibold, PingFang SC;
  font-weight: 600;
}