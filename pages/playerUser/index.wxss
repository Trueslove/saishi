.player-user-page {
  min-height: 100vh;
  background: #F7F7F7;
  padding-bottom: 190rpx;
  
}


.player-user-page .user-wrap {
  background: #fff;
  margin: 0 24rpx;
  border-radius: 16rpx;
}

.player-user-page .user-wrap .user {
  margin: 0 28rpx;
  padding: 22rpx 0;
  display: flex;
  align-items: center;
  padding-bottom: 22rpx;
  border-bottom: 1px solid rgba(0, 0, 0, 0.1);
}

.player-user-page .user-wrap .work {
  display: flex;
  align-items: center;
  padding: 0 28rpx;
  margin-top: 14rpx;
  padding-bottom: 14rpx;

}

.player-user-page .user-wrap .work image {
  width: 30rpx;
  height: 28rpx;
  margin-right: 12rpx;
}

.player-user-page .user-wrap .work text {
  font-size: 24rpx;
  font-family: PingFang SC-Regular, PingFang SC;
  font-weight: 400;
  color: #121314;
}

.player-user-page .user-wrap .work text:nth-of-type(2) {
  padding-left: 20rpx;
  flex: 1;
}

.player-user-page .user-wrap .work button {
  width: 140rpx;
  height: 56rpx;
  line-height: 56rpx;
  background: #F8F8F8;
  border-radius: 8rpx;
  padding: 0;
  font-size: 24rpx;
  font-family: PingFang SC-Medium, PingFang SC;
  font-weight: 500;
  color: #121314;
}

.player-user-page .user-wrap .user .info {
  display: flex;
  flex-direction: column;
  margin-left: 20rpx;
  flex: 1;
}

.player-user-page .user-wrap .user .info text:first-of-type {
  font-size: 34rpx;
  font-family: PingFang SC-Medium, PingFang SC;
  font-weight: 500;
  color: #121314;
}

.player-user-page .user-wrap .user .info text:last-of-type {
  font-size: 24rpx;
  font-family: PingFang SC-Regular, PingFang SC;
  font-weight: 400;
  color: #B2B2B2;
}

.player-user-page .user-wrap .user .info navigator {
  font-size: 24rpx;
  font-family: PingFang SC-Regular, PingFang SC;
  font-weight: 400;
  color: #121314;
}

.player-user-page .user-wrap .user image {
  width: 84rpx;
  height: 84rpx;
  border-radius: 50%;
}

.player-user-page .cover {
  width: 702rpx;
  margin: 0 auto;
  margin-top: 16rpx;
  border-top-left-radius: 10rpx;
  border-top-right-radius: 10rpx;
  overflow: hidden;
  display: flex;
}

.player-user-page .cover image {
  width: 100%;
}



.player-user-page .info-wrap {
  background: #fff;
  margin: 0 24rpx;
  width: 702rpx;
  border-bottom-left-radius: 10rpx;
  border-bottom-right-radius: 10rpx;
  margin-bottom: 16rpx;
  padding-bottom: 20rpx;
  margin-right: unset !important;
}

.player-user-page .work-info {
  display: flex;
  align-items: center;
  margin: 0 32rpx;
  padding-top: 16rpx;
  padding-bottom: 30rpx;
  border-bottom: 1px solid rgba(0, 0, 0, 0.1);
}

.player-user-page .work-info .left {
  display: flex;
  flex-direction: column;
  flex: 1;
}

.player-user-page .work-info .left text:first-of-type {
  font-size: 34rpx;
  font-family: PingFang SC-Medium, PingFang SC;
  font-weight: 500;
  color: #121314;
}

.player-user-page .work-info .left text:last-of-type {
  font-size: 20rpx;
  font-family: PingFang SC-Regular, PingFang SC;
  font-weight: 400;
  color: #B2B2B2;
}

.player-user-page .work-info .right {
  display: flex;
  flex-direction: column;

}

.not-marginTop {
  margin-top: 0 !important;
}

.player-user-page .work-info .right text:first-of-type {
  font-size: 20rpx;
  font-family: PingFang SC-Regular, PingFang SC;
  font-weight: 400;
  color: #B2B2B2;
}

.player-user-page .work-info .right text:last-of-type {
  font-size: 40rpx;
  font-family: DIN-Bold, DIN;
  font-weight: bold;
  color: #121314;

}


.work-msg {
  padding: 0 28rpx;
  margin-right: 28rpx;
}

.work-msg .title {
  font-size: 20rpx;
  font-family: PingFang SC-Medium, PingFang SC;
  font-weight: 500;
  color: #B2B2B2;
  margin-top: 20rpx;
  display: block;
}

.work-msg text {
  font-size: 24rpx;
  font-family: PingFang SC-Regular, PingFang SC;
  font-weight: 400;
  color: #121314;
  display: inline-block;
  padding-top: 12rpx;
}


.player-user-page .video {
  margin-top: 12rpx;
  width: 646rpx;
  height: 396rpx;
  border-radius: 10rpx;
  overflow: hidden;
  position: relative;
}

.player-user-page .video video {
  width: 100%;
  height: 100%;
  border-radius: 10rpx;
}

.player-user-page .video image {
  position: absolute;
  top: 50%;
  left: 50%;
  width: 90rpx;
  height: 90rpx;
  transform: translate(-50%, -50%);
}

.player-user-page .school {
  padding: 26rpx 28rpx;
  background: #fff;
  display: flex;
  align-items: center;
  border-bottom: 1px solid #EFEFEF;
  border-radius: 18rpx;
  border-bottom-left-radius: 0;
  border-bottom-right-radius: 0;
}

.player-user-page .school .info {

  display: flex;
  flex-direction: column;
  flex: 1;

}

.player-user-page .school .info text:first-of-type {
  font-size: 34rpx;
  font-family: PingFang SC-Medium, PingFang SC;
  font-weight: 500;
  color: #121314;
}

.player-user-page .school .info text:last-of-type {
  font-size: 26rpx;
  font-family: PingFang SC-Medium, PingFang SC;
  font-weight: 500;
  color: #121314;
}

.player-user-page .school image {
  width: 123rpx;
  height: 103rpx;
}

.player-user-page .school-wrap {
  margin: 0 26rpx;
  background: #fff;
  border-bottom-right-radius: 16rpx;
  border-bottom-left-radius: 16rpx;

}

.player-user-page .school-hot {
  display: flex;
  padding: 24rpx 0;
  margin: 0 26rpx;
  border-bottom: 1px solid #EFEFEF;
}


.player-user-page .school-hot image {
  width: 96rpx;
  height: 96rpx;
  margin-right: 14rpx;
}

.player-user-page .school-hot .info {
  flex: 1;
  padding: 10rpx 0;
}


.player-user-page .school-hot .info .name {
  display: flex;
  align-items: center;
  margin-bottom: 30rpx;
}

.player-user-page .school-hot .info .name text {
  font-size: 24rpx;
  font-family: PingFang SC-Medium, PingFang SC;
  font-weight: 500;
  color: #121314;
}

.player-user-page .school-hot .info .name .success {
  color: var(--themeColor);
  margin-left: 22rpx;
}

.player-user-page .school-hot .info .bottom {
  font-size: 24rpx;
  font-family: PingFang SC-Regular, PingFang SC;
  font-weight: 400;
  color: #858585;
}

.player-user-page .review-infor {
  padding: 0 28rpx;
  padding-top: 40rpx;
  
}

.player-user-page .review-infor .title {
  display: flex;
  align-items: center;
}

.player-user-page .review-infor .title text {
  font-size: 34rpx;
  font-family: PingFang SC-Medium, PingFang SC;
  font-weight: 500;
  color: #121314;
  flex: 1;
}

.player-user-page .review-infor .title text text {
  color: var(--themeColor);
  margin-right: 10rpx;
}

.player-user-page .review-infor .title .right text:first-of-type {
  font-size: 24rpx;
  font-family: PingFang SC-Semibold, PingFang SC;
  font-weight: 600;
  color: var(--themeColor);
}

.player-user-page .review-infor .title .right text:last-of-type {
  font-size: 40rpx;
  font-family: DIN-Bold, DIN;
  font-weight: bold;
  color: #121314;
  padding-left: 8rpx;
}

.review-infor-wrap {
  display: flex;
  padding-top: 36rpx;
}

.review-infor-wrap image {
  width: 96rpx;
  height: 96rpx;
  border-radius: 50rpx;
}

.review-infor-wrap .info {
  flex: 1;
  margin-left: 24rpx;
}

.review-infor-wrap .info .info-top {
  font-size: 24rpx;
  font-family: PingFang SC-Semibold, PingFang SC;
  font-weight: 600;
  color: #121314;
  flex: 1;
  display: flex;
  align-items: center;
}

.review-infor-wrap .info .info-top text {
  font-size: 24rpx;
  font-family: PingFang SC-Semibold, PingFang SC;
  font-weight: 600;
  color: #121314;
  flex: 1;
}

.review-infor-wrap .info .info-top .info-top-right {
