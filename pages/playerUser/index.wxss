  align-items: flex-end;
  height: 42rpx;
  line-height: 42rpx;
}

.review-infor-wrap .info .info-top .info-top-right text {
  font-size: 48rpx;
  font-family: DIN-Bold, DIN;
  font-weight: bold;
  color: #121314;
}

.review-infor-wrap .info .info-top .info-top-right image {
  width: 28rpx;
  height: 28rpx;
  margin-left: 2rpx;
}

.review-infor-wrap .info .info-wrap {
  flex-direction: column;
  display: flex;
  align-items: flex-end;
  width: unset !important;

}

.review-infor-wrap .info .info-wrap text {
  font-size: 26rpx;
  font-family: PingFang SC-Regular, PingFang SC;
  font-weight: 400;
  color: #121314;
  margin-top: 22rpx;
}

.review-infor-wrap .info .info-wrap image {
  margin-top: 10rpx;
  width: 200rpx;
  height: 111rpx;
}

.review-infor-wrap .child-score {
  display: flex;
  align-items: center;
  width: 100%;
  justify-content: center;
  margin: 0 20rpx;
  padding-bottom: 20rpx;
  position: relative;
  padding-right: 27rpx;
}

.review-infor-wrap .child-score::before {
  content: '';
  position: absolute;
  bottom: 0;
  left: -141rpx;
  right: 28rpx;
  width: 654rpx;
  height: 6rpx;
  background: #F7F7F7;

}

.review-infor-wrap .child-score text {
  font-size: 20rpx;
  font-family: PingFang SC-Regular, PingFang SC;
  font-weight: 400;
  color: #B2B2B2;
}

.review-infor-wrap .child-score text::before,
.review-infor-wrap .child-score text::after {
  content: '';
  position: absolute;
  left: 0;
  top: 14rpx;
  width: 194rpx;
  height: 1rpx;
  background: #B2B2B2;

}

.review-infor-wrap .child-score text::after {
  right: 28rpx;
  left: unset;
  width: 192rpx;
}
.review-infor-wrap .child-score-bottom image {
  transform: rotate(180deg);
}

.review-infor-wrap .child-score image {
  margin-left: 4rpx;
  width: 16rpx;
  height: 10rpx;
}

.review-infor-wrap .evaluate {
  display: flex;
  flex-direction: column;
  margin-bottom: 30rpx;
  margin-top: 30rpx;
}

.review-infor-wrap .evaluate .evaluate-item {
  display: flex;

  align-items: center;
}

.review-infor-wrap .evaluate .evaluate-item text:first-of-type {
  font-size: 26rpx;
  font-family: PingFang SC-Medium, PingFang SC;
  font-weight: 500;
  color: #121314;
  flex: 1;
  display: inline-block;
}

.review-infor-wrap .evaluate .evaluate-item text:last-of-type {
  font-size: 26rpx;
  font-family: PingFang SC-Medium, PingFang SC;
  font-weight: 500;
  color: #162246;
}

.review-infor-wrap .evaluate .msg {
  font-size: 24rpx;
  font-family: PingFang SC-Regular, PingFang SC;
  font-weight: 400;
  color: #858585;
  padding-top: 10rpx;
}


