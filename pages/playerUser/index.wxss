  justify-content: flex-end;
  flex: 1;
}

.player-user-page .nav-bottom .bottom .right button {
  height: 68rpx;
  line-height: 68rpx;
  width: unset;
  padding: 0 26rpx;
  font-size: 24rpx;
  font-family: PingFang SC-Regular, PingFang SC;
  font-weight: 400;
  display: flex;
  align-items: center;
  color: #535660;
